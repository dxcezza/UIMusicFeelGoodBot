services:
  - type: web
    name: music-library
    env: python
    buildCommand: pip install -r requirements.txt && npm install && npm run build
    startCommand: python app.py
    envVars:
      - key: PYTHON_VERSION
        value: 3.11.0
      - key: NODE_VERSION
        value: 18
    healthCheckPath: /
    disk:
      name: temp
      mountPath: /temp
      sizeGB: 1
